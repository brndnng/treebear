# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'treebear' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for treebear
  pod 'Hero'
  pod 'ARCL'
  pod 'GoogleSignIn'
  target 'treebearTests' do
    inherit! :search_paths
    use_frameworks!
    # Pods for testing
    pod 'Hero'
    pod 'ARCL'
    pod 'GoogleSignIn'
  end

  target 'treebearUITests' do
    use_frameworks!
    inherit! :search_paths
    # Pods for testing
    pod 'Hero'
    pod 'ARCL'
    pod 'GoogleSignIn'
  end

end
